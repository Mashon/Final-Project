<ul class="collapsible" data-collapsible="accordion">

<!-- Left Header Navbar -->
<li>Project Name <!-- link to home if not logged in, and profile if logged in --> </li>

<!-- Center Navbar -->
<li> our logo centered here </li>


<!-- Sign Up Section -->
  <% if user_signed_in? %>
    <li><div class="navright"><%= link_to 'Profile', edit_user_registration_path %></div></li>
  <% else %>
    <li>
      <div class="collapsible-header">SignUp</div>
      <div class="collapsible-body"><p><%= render '/user/new' %></p></div>
    </li>
  <% end %>

<!-- Login Section -->
  <% if user_signed_in? %>
    <li><div class="navright"><%= link_to('Logout', destroy_user_session_path, :method => :delete) %> |</div></li>
  <% else %>
    <li>
      <div class="collapsible-header">Login</div>
      <div class="collapsible-body"><p><%= render '/user/login' %></p></div>
    </li>
  <% end %>

</ul>
